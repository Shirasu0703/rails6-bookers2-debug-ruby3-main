<h1>Follow User</h1>
<table class="table table-hover table-inverse">
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |users| %>
      <tr>
        <td><%= image_tag users.get_profile_image, size:'50x50' %></td>
        <td><%= current_user.name %></td>
        <td>フォロー数：<%= users.followings.count %></td>
        <td>フォロワー数：<%= users.followers.count %></td>
        <td><%= link_to 'Show', users %></td>
      </tr>
    <% end %>
  </tbody>
</table>